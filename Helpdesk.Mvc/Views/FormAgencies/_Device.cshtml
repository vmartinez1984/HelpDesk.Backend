@{
    Layout= null;
}
<h6 class="text-primary">Dispositivos</h6>
<div class="row">
    <div class="col-8">
        <div class="form-group mb-3">
            <label class="control-label">Nombre o número de serie</label>
            <input id="searchDevice" class="form-control" maxlength="50" placeholder="Nombre o número de serie" onkeyup="onkeyup_searchDevice(event);"/>
        </div>
    </div>
    <div class="col-4">
        <div class="form-group mb-3">
            <br/>
            <label class="control-label"></label>
            <button type="button" class="btn btn-primary" onclick="onclick_searchDevice();">
                <i class="align-middle" data-feather="search"></i> 
                <span class="align-middle">Buscar</span>                            
            </button>
        </div>
    </div>
</div>
<div id="listDevices" class="border border-primary p-2"></div>

<h6 class="text-info mt-3">Dispositivos agregados a la agencia</h6>
<div id="listDevicesAdds" class="border border-info p-2"></div>